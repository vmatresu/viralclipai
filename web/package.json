{
  "name": "viralvideoai-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:docker": "docker-compose -f ../docker-compose.dev.yml up -d web && docker-compose -f ../docker-compose.dev.yml logs -f web",
    "build": "next build",
    "build:docker": "docker-compose -f ../docker-compose.dev.yml build web",
    "start": "next start",
    "start:docker": "docker-compose -f ../docker-compose.dev.yml up -d web",
    "stop:docker": "docker-compose -f ../docker-compose.dev.yml stop web",
    "logs:docker": "docker-compose -f ../docker-compose.dev.yml logs -f web",
    "lint": "next lint",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "firebase": "^9.23.0",
    "swr": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.4.0",
    "prettier": "^3.3.0"
  }
}
