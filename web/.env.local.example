# Next.js local development env example
# Copy to web/.env.local and fill in with your Firebase web config.

# Local API (docker-compose.dev.yml maps api to localhost:8000)
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000

# Firebase Web SDK (from your Firebase console)
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_web_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_firebase_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=000000000000
NEXT_PUBLIC_FIREBASE_APP_ID=1:000000000000:web:xxxxxxxxxxxxxxxx
